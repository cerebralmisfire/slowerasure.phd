@book{deleon2015,
  title = {The {{Land}} of {{Open Graves}}: {{Living}} and {{Dying}} on the {{Migrant Trail}}},
  author = {De Le{\'o}n, Jason},
  year = {2015},
  publisher = {University of California Press, CA},
  urldate = {2025-08-08},
  abstract = {{$<$}strong{$>$}In this gripping and provocative "ethnography of death," anthropologist and MacArthur "Genius" Fellow Jason De Le{\dbend}n sheds light on one of the most pressing political issues of our time-the human consequences of US immigration and border policy.{$<$}/strong{$>$}  \emph{The Land of Open Graves}  reveals the suffering and deaths that occur daily in the Sonoran Desert of Arizona as thousands of undocumented migrants attempt to cross the border from Mexico into the United States. Drawing on the four major fields of anthropology, De Le{\dbend}n uses an innovative combination of ethnography, archaeology, linguistics, and forensic science to produce a scathing critique of "Prevention through Deterrence," the federal border enforcement policy that encourages migrants to cross in areas characterized by extreme environmental conditions and high risk of death. For two decades, systematic violence has failed to deter border crossers while successfully turning the rugged terrain of southern Arizona into a killing field. Featuring stark photography by Michael Wells, this book examines the weaponization of natural terrain as a border wall: first-person stories from survivors underscore this fundamental threat to human rights, and the very lives, of non-citizens as they are subjected to the most insidious and intangible form of American policing as institutional violence. In harrowing detail, De Le{\dbend}n chronicles the journeys of people who have made dozens of attempts to cross the border and uncovers the stories of the objects and bodies left behind in the desert.  \emph{The Land of Open Graves}  will spark debate and controversy.},
  isbn = {978-0-520-28274-2}
}

