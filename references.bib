@phdthesis{notwell2022,
  title = {Decolonization Is an {{Everyday Struggle}} in  {{Palestine}}: {{A Collective Practice}} of {{Freedom Rooted}} in {{Decolonial Love}}},
  author = {Notwell, Jess},
  year = {2022},
  abstract = {This study mobilizes the Indigenous Methodology of Decolonial Co-Resistance, within the praxis framework of Intersectional Decoloniality, to highlight that Palestinian women frontliners are building the knowledges and modelling the actions necessary to liberate Palestine and create a decolonized world by enacting decolonial love, hope, beloved community and co-liberation. Intersectional Decoloniality centres the stories of 33 Palestinian women in different socio-political and urban/village/refugee camp locations to make visible their decolonial action against Israeli settler colonialism and for Palestinian futurities. Decolonial Co-Resistance is a methodology of gathering, analyzing and mobilizing those stories, toward co-liberation, through co-resistance actions and narrating lifetimes of struggle. This research foregrounds decolonial love of Palestinian-ness, Palestinian community, ancestors and the land, as the foundation of women's decolonial action. Next, it details how the Palestinian women who participated in this study mobilize decolonial visions as the hope which sustains the struggle. Together, hope and love allow the co-creation of beloved community as a pathway to healing colonial fragmentation of Palestinians and  Palestine. Finally this research explores direct action for co-liberation, as women's organizing based on community priorities and protection of   al-Aqsa Mosque, and as a way to both prefigure and incrementally achieve the relations of freedom necessary to liberate and decolonize Palestine.},
}

@article{novaco2002,
